    "express": "^4.19.2",
    "log4js": "^6.9.1",
    "prisma": "^5.18.0",
    "tsoa": "^6.4.0",
    "yaml": "^2.5.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
